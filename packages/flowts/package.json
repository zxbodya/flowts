{
  "name": "flowts",
  "version": "0.15.8",
  "description": "Utility script to convert flow typed code to typescript",
  "bin": {
    "flowts": "./bin/flowts.js",
    "flowts-uncomment-flow-types": "./bin/flowts-uncomment-flow-types.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "cover": "jest --coverage",
    "test": "jest",
    "clean": "rimraf dist",
    "prepublishOnly": "yarn run clean && tsc -b --force",
    "build": "tsc -b"
  },
  "files": [
    "bin/*",
    "dist/*",
    "readme.md"
  ],
  "keywords": [],
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/zxbodya/flowts/tree/master/packages/cli",
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-flow-strip-types": "^7.17.12",
    "@babel/plugin-transform-typescript": "^7.18.1",
    "@babel/preset-flow": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/traverse": "^7.18.2",
    "@babel/types": "^7.18.2",
    "@flowts/babel-plugin-recast": "^0.15.8",
    "@zxbodya/babel-plugin-flow-to-typescript": "^0.15.8",
    "@zxbodya/recast": "0.21.2",
    "babel-plugin-typescript-to-flow": "^0.15.8",
    "commander": "^9.3.0",
    "globby": "^11.0.3",
    "jest-diff": "^28.1.0",
    "ora": "^5.4.1",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.21",
    "typescript": "^4.7.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/preset-env": "^7.18.2",
    "@types/babel__core": "^7.1.18",
    "@types/babel__traverse": "7.17.1",
    "@types/jest": "^27.5.1",
    "@types/ora": "^3.2.0",
    "@types/prettier": "^2.6.3",
    "jest": "^28.1.0",
    "rimraf": "^3.0.2",
    "tsm": "^2.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "dist"
    ]
  }
}
