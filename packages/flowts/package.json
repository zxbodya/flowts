{
  "name": "flowts",
  "version": "0.15.15",
  "description": "Utility script to convert flow typed code to typescript",
  "bin": {
    "flowts": "./bin/flowts.js",
    "flowts-uncomment-flow-types": "./bin/flowts-uncomment-flow-types.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "cover": "jest --coverage",
    "test": "jest",
    "clean": "rimraf dist",
    "prepublishOnly": "yarn run clean && tsc -b --force",
    "build": "tsc -b"
  },
  "files": [
    "bin/*",
    "dist/*",
    "readme.md"
  ],
  "keywords": [],
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/zxbodya/flowts/tree/master/packages/cli",
  "dependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/traverse": "^7.18.13",
    "@babel/types": "^7.18.13",
    "@flowts/babel-plugin-recast": "^0.15.15",
    "@zxbodya/babel-plugin-flow-to-typescript": "^0.15.15",
    "@zxbodya/recast": "0.21.4",
    "commander": "^9.4.0",
    "globby": "^11.0.3",
    "jest-diff": "^29.0.2",
    "ora": "^5.4.1",
    "prettier": "^2.7.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-flow-strip-types": "^7.18.9",
    "@babel/plugin-transform-typescript": "^7.18.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@types/babel__core": "^7.1.18",
    "@types/babel__traverse": "7.18.1",
    "@types/jest": "^29.0.0",
    "@types/prettier": "^2.7.0",
    "jest": "^29.0.2",
    "rimraf": "^3.0.2",
    "tsm": "^2.2.2",
    "source-map-support": "^0.5.21",
    "typescript": "^4.8.2"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "dist"
    ]
  }
}
