{
  "name": "@zxbodya/babel-plugin-flow-to-typescript",
  "description": "Babel plugin to convert Flow into TypeScript",
  "license": "MIT",
  "files": [
    "dist/*",
    "README.md"
  ],
  "version": "0.10.7",
  "scripts": {
    "cover": "jest --coverage",
    "report-coverage": "codecov",
    "test": "jest",
    "clean": "rimraf dist",
    "prepublishOnly": "yarn run clean && tsc -b --force",
    "build": "tsc -b"
  },
  "keywords": [
    "babel",
    "flow",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/zxbodya/flowts/tree/master/packages/babel-plugin-flow-to-typescript",
  "peerDependencies": {
    "@babel/core": "^7.4.4"
  },
  "dependencies": {
    "@babel/template": "^7.8.3",
    "@babel/traverse": "^7.8.4",
    "@babel/types": "7.9.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "7.0.9",
    "@types/jest": "^25.2.2",
    "@zxbodya/recast": "^0.18.9",
    "codecov": "^3.6.5",
    "jest": "^26.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
