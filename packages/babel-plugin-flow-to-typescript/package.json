{
  "name": "@zxbodya/babel-plugin-flow-to-typescript",
  "description": "Babel plugin to convert Flow into TypeScript",
  "license": "MIT",
  "files": [
    "dist/*",
    "README.md"
  ],
  "version": "0.11.2",
  "scripts": {
    "cover": "jest --coverage",
    "report-coverage": "codecov",
    "test": "jest",
    "clean": "rimraf dist",
    "prepublishOnly": "yarn run clean && tsc -b --force",
    "build": "tsc -b"
  },
  "keywords": [
    "babel",
    "flow",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/zxbodya/flowts/tree/master/packages/babel-plugin-flow-to-typescript",
  "peerDependencies": {
    "@babel/core": "^7.10.4"
  },
  "dependencies": {
    "@babel/template": "^7.10.4",
    "@babel/traverse": "^7.11.5",
    "@babel/types": "^7.11.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/babel__traverse": "7.0.15",
    "@types/jest": "^26.0.14",
    "@zxbodya/recast": "^0.18.9",
    "codecov": "^3.7.2",
    "jest": "^26.4.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "dist"
    ]
  }
}
